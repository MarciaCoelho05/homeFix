{
  "version": 2,
  "buildCommand": "npm install",
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/server.js"
    }
  ]
}
