{
  "version": 2,
  "builds": [
    {
      "src": "homefix-backend/src/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "homefix-backend/prisma/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "homefix-backend/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "homefix-backend/src/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
